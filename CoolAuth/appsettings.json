{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "Database": "${DB_CONNECTION_STRING}",
    "Redis": "${REDIS_CONNECTION_STRING}"
  },
  "Redis": {
    "InstanceName": "CoolAuth"
  },
  "JwtSettings": {
    "Issuer": "CoolAuth",
    "Key": "${JWT_SIGN_KEY}"
  },
  "AllowedHosts": "*"
}
